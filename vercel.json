{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "env": {
    "SKIP_ENV_VALIDATION": "1",
    "DISABLE_ESLINT": "1",
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "build": {
    "env": {
      "SKIP_ENV_VALIDATION": "1",
      "DISABLE_ESLINT": "1"
    }
  },
  "functions": {
    "app/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  }
}
